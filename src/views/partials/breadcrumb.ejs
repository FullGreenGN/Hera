<nav class="flex" aria-label="Breadcrumb">
    <ol class="inline-flex items-center space-x-1 md:space-x-2 rtl:space-x-reverse">
        <% if(breadcrumbs) { %>
            <% breadcrumbs.forEach((breadcrumb, index) => { %>
                <li>
                    <div class="flex items-center">
                        <% if(index === breadcrumbs.length - 1) { %>
                            <a href="<%= breadcrumb.link %>" class="text-sm font-medium text-gray-700 md:-ms-2 dark:text-gray-400"><%= breadcrumb.name %></a>
                        <% } else { %>
                            <i class="<%= breadcrumb.icon %> w-3 h-3 me-2.5 text-gray-400 mx-1"></i>
                            <a href="<%= breadcrumb.link %>" class="ms-1 text-sm font-medium text-gray-700 hover:text-blue-600 md:ms-2 dark:text-gray-400 dark:hover:text-gray-600"><%= breadcrumb.name %></a>
                            <i class="fas fa-chevron-right rtl:rotate-180 w-3 h-3 text-gray-400 mx-1"></i>
                        <% } %>
                    </div>
            <% })%>
        <% } %>
    </ol>
</nav>